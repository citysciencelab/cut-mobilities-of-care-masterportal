{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ["./devtools/jsdoc", "./js", "./modules", "./addons"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(node_modules\\/)|(addons(\\/|\\\\).*(\\/|\\\\)node_modules(\\/|\\\\))|((^|\\/|\\\\)__)"
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    },
    "linenums": true,
    "opts": {
      "encoding": "utf8",
      "recurse": true,
      "destination": "./jsdoc",
      "template": "node_modules/jaguarjs-jsdoc"
    }
}
